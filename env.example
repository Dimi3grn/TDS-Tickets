# Discord Bot Configuration
# Copy this file to .env and fill in your values

# Bot token from Discord Developer Portal
DISCORD_TOKEN=your_bot_token_here

# Your bot's application/client ID
CLIENT_ID=your_client_id_here

# Your development server ID (right-click server -> Copy Server ID)
GUILD_ID=your_dev_server_id_here

# Channel IDs (set these after creating channels in your server)
TICKET_CHANNEL_ID=channel_where_ticket_button_appears
CARRY_PROOF_CHANNEL_ID=channel_for_carry_proof_posts
TICKET_CATEGORY_ID=category_where_ticket_channels_are_created

# Role IDs (set these after creating roles in your server)
CARRY_HELPER_ROLE_ID=role_id_for_carry_helpers
MOD_ROLE_ID=role_id_for_moderators
